{
  "name": "chatgpt",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "ts-node src/index.ts",
    "launch": "export CHATGPT_TOOL_OPEN_BROWSER=true && yarn run start:dev",
    "build": "tsc",
    "install-binary": "yarn install && (tsc || true) && sudo ln -sf $(pwd)/dist/index.js /usr/local/bin/chatgpt && sudo chmod +x /usr/local/bin/chatgpt",
    "run-built": "node src/index.js"
  },
  "dependencies": {
    "clipboardy": "^3.0.0",
    "js-yaml": "^4.1.0",
    "open": "^8.4.1",
    "prettyjson": "^1.2.5",
    "ts-clipboard": "^1.0.17",
    "typescript": "^4.9.5",
    "yaml": "^2.2.1",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.13.0",
    "@types/yargs": "^17.0.22",
    "ts-node": "^10.9.1"
  }
}
